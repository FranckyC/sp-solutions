{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "WebhookProvisioning": {
            "matchCondition": {
                "route": "/api/webhookHandler",
                "methods": [
                    "POST"
                ]
            },
            "backendUri": "http://%WEBSITE_HOSTNAME%/api/webhookHandler"
        }
    }
}